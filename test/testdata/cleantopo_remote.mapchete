process: ../example_process.py
zoom_levels:
    min: 0
    max: 4
pyramid:
    grid: geodetic
input:
    file1:
        format: TileDirectory
        path: https://ungarj.github.io/mapchete_testdata/tiled_data/raster/cleantopo
        metatiling: 1
        grid: geodetic
        extension: tif
        dtype: uint16
        count: 1
output:
    dtype: uint16
    bands: 1
    format: GTiff
    path: tmp/cleantopo_remote
    pixelbuffer: 20
